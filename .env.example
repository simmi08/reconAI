# DB must follow /Users/saumyas/personalProjects/README.md
DATABASE_URL=postgresql://<user>:<pass>@localhost:5432/<db>
# Optional migration-only URL (recommended for Supabase direct DB host)
DATABASE_URL_MIGRATE=

# Required Supabase storage integration:
# - Ingestion scans from raw bucket
# - Processed artifacts are written to storage bucket
SUPABASE_URL=
SUPABASE_SECRET_KEY=
# Legacy fallback if secret key is not available:
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_RAW_BUCKET=
SUPABASE_STORAGE_BUCKET=
SUPABASE_RAW_PREFIX=
SUPABASE_STORAGE_PREFIX=

GEMINI_API_KEY=YOUR_KEY
# prefer preview, but allow override
GEMINI_MODEL=gemini-3-flash-preview

PROCESS_BATCH_SIZE=25
CONFIDENCE_THRESHOLD=0.75
AMOUNT_TOLERANCE_PCT=0.02
